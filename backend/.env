DB_HOST=127.0.0.1   # ใช้ IP นี้แทน 'localhost'
DB_USER=root        # ใช้ root ตามที่ตั้งไว้ใน Workbench
DB_PASSWORD=        # ถ้ามีรหัสผ่านให้ใส่ ถ้าไม่มีให้ปล่อยว่าง
DB_NAME=etour       # ตรวจสอบว่าฐานข้อมูลนี้มีอยู่จริง
DB_PORT=3306        # ใช้พอร์ต 3306 (เพราะใน Workbench ใช้พอร์ตนี้)
JWT_SECRET=my_secure_random_key
